	</pageFooter>
	<summary>
		<band height="318" splitType="Stretch">
			<barChart>
				<chart>
					<reportElement x="32" y="22" width="483" height="279"/>
					<chartTitle/>
					<chartSubtitle/>
					<chartLegend/>
				</chart>
				<categoryDataset>
					<categorySeries>
						<seriesExpression><![CDATA[$F{estadoInformeFinalCurso}]]></seriesExpression>
						<categoryExpression><![CDATA[$F{estadoInformeFinalCurso}]]></categoryExpression>
						<valueExpression><![CDATA[$F{total}]]></valueExpression>
					</categorySeries>
				</categoryDataset>
				<barPlot>
					<plot/>
					<itemLabel color="#000000" backgroundColor="#FFFFFF"/>
					<categoryAxisFormat>
